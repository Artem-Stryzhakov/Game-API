{
  "openapi": "3.0.2",
  "info": {
    "title": "Games-API",
    "version": "1.0.0",
    "description": "### This is a Swagger UI for our Games-API"
  },
  "paths": {
    "/games": {
      "get": {
        "tags": [
          "games"
        ],
        "responses": {
          "304": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "examples": {
                  "List of games": {
                    "value": [
                      "Witcher 3",
                      "For Honor",
                      "Metro EXODUS"
                    ]
                  }
                }
              }
            }
          }
        },
        "description": "Returns a list of all games in API"
      }
    }
  }
}